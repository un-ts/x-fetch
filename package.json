{
  "name": "x-fetch",
  "version": "0.2.3",
  "type": "module",
  "description": "A simple but elegant `fetch` API wrapper, use `fetch` like a charm",
  "repository": "git+https://github.com/un-ts/x-fetch.git",
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "funding": "https://opencollective.com/x-fetch",
  "license": "MIT",
  "packageManager": "yarn@4.8.1",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "./lib/index.cjs",
  "types": "./index.d.cts",
  "module": "./lib/index.js",
  "exports": {
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "module-sync": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "require": {
      "types": "./index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "files": [
    "index.d.cts",
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "api",
    "fetch",
    "fetch-api",
    "node-fetch"
  ],
  "scripts": {
    "build": "concurrently -r 'yarn:build:*'",
    "build:r": "r -f cjs",
    "build:tsc": "tsc -p src",
    "clean": "rimraf -g .type-coverage coverage dist lib '.*cache'",
    "dev": "vitest",
    "docs": "vite",
    "docs:build": "vite build",
    "format": "prettier --write .",
    "lint": "concurrently -r 'yarn:lint:*'",
    "lint:es": "eslint . --cache --max-warnings 10",
    "lint:style": "stylelint . --cache",
    "lint:tsc": "tsc --noEmit",
    "prepare": "simple-git-hooks && yarn-berry-deduplicate || exit 0",
    "release": "yarn build && clean-pkg-json && changeset publish",
    "serve": "vite preview",
    "test": "vitest run",
    "typecov": "type-coverage"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@1stg/browserslist-config": "^2.1.2",
    "@1stg/common-config": "^12.0.0",
    "@1stg/stylelint-config": "^6.0.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@edge-runtime/vm": "^5.0.0",
    "@mdx-js/rollup": "^3.1.0",
    "@pkgr/rollup": "^6.0.1",
    "@shikijs/rehype": "^3.2.1",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/web": "^0.0.216",
    "@vercel/analytics": "^1.5.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "clean-pkg-json": "^1.2.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "github-markdown-css": "^5.8.1",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.12.1",
    "size-limit": "^11.2.0",
    "stylelint": "^16.18.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5.8.3",
    "vite": "^6.0.14",
    "vitest": "^3.1.1",
    "yarn-berry-deduplicate": "^6.1.1"
  },
  "resolutions": {
    "es5-ext": "npm:@unes/es5-ext@latest",
    "prettier": "^3.5.3"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1KB"
    }
  ],
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreAsAssertion": true,
    "ignoreFiles": [
      "**/*.d.ts"
    ],
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "strict": true,
    "update": true
  }
}
